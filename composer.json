{
    "name": "nitsan/ns-pwa",
    "type": "typo3-cms-extension",
    "description": "Make your TYPO3 website lightning fast & work offline! Get our TYPO3 PWA extension for better SEO & mobile experience. Install today!",
    "homepage": "https://extensions.typo3.org/extension/ns_pwa/",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3 PWA",
        "TYPO3 Progressive Web Application",
        "TYPO3 PWA Sites",
        "TYPO3"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_pwa/issues"
      },
    "require": {
        "typo3/cms-core": "^11 || ^12"
    },      
    "autoload": {
        "psr-4": {
            "NITSAN\\NsPwa\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-pwa": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_pwa"
        }
    }
}
