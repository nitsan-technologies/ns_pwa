{
    "name": "nitsan/ns-pwa",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11 || ^12"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsPwa\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-pwa": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_pwa"
        }
    }
}
